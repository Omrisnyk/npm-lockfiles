{
  "name": "@snyk/vuln",
  "description": "Snyk's vulnerability module",
  "main": "dist/index.js",
  "engines": {
    "node": ">=8"
  },
  "scripts": {
    "build": "tsc",
    "build-dev": "tsc -w",
    "lint": "eslint lib && tslint -p tsconfig.json -e '**/*.js'",
    "check-only": "! grep 'test.only' test/*.test.js -n",
    "test": "npm run check-only && npm run lint && CI=1 tap test/*.test.js -R spec --timeout 390",
    "watch": "nodemon -q -i dist -x 'npm run build; for FILE in test/*.test.js; do echo $FILE; tape $FILE | tap-spec; if [ $? -ne 0 ]; then exit 1; fi; done'",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post",
    "prepare": "npm run snyk-protect; npm run build",
    "snyk-protect": "snyk protect"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/snyk/vuln.git"
  },
  "keywords": [
    "vulnerability"
  ],
  "author": "snyk.io",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/snyk/vuln/issues"
  },
  "homepage": "https://github.com/snyk/vuln",
  "dependencies": {
    "@snyk/deb-semver": "^1.0.2",
    "@snyk/gemfile": "^1.0.0",
    "@snyk/log": "^2.5.11",
    "@snyk/maven-semver": "1.2.0",
    "@snyk/rpm-semver": "1.0.0",
    "@snyk/ruby-semver": "2.0.0",
    "@snyk/vulndb-client": "2.0.0",
    "async": "^2.0.1",
    "debug": "^3.1.0",
    "eslint": "^4.17.0",
    "fast-memoize": "^2.1.1",
    "graphlib": "^2.1.1",
    "hosted-git-info": "^2.1.5",
    "itercol": "^1.0.3",
    "lodash": "^4.15.0",
    "nock": "^9.2.6",
    "object-hash": "^1.1.4",
    "octonode": "^0.9.3",
    "p-map": "^1.2.0",
    "pom-parser": "^1.1.1",
    "request": "^2.87.0",
    "request-promise-native": "^1.0.5",
    "semver": "^5.5.0",
    "sleep-promise": "^2.0.0",
    "snyk": "^1.105.0",
    "snyk-config": "^2.2.0",
    "snyk-module": "^1.8.1",
    "snyk-policy": "^1.10.2",
    "source-map-support": "^0.5.5",
    "undefsafe": "^1.3.1",
    "xml2js": "^0.4.19"
  },
  "devDependencies": {
    "@types/graphlib": "^2.1.4",
    "@types/ioredis": "^3.2.8",
    "@types/lodash": "^4.14.106",
    "@types/node": "^8.9.0",
    "@types/object-hash": "^1.2.0",
    "@types/p-map": "^1.1.0",
    "@types/semver": "^5.5.0",
    "ioredis": "^2.4.0",
    "ioredis-mock": "^3.8.1",
    "proxyquire": "^1.7.10",
    "semantic-release": "^8.0.0",
    "sinon": "^6.0.0",
    "tap": "^12.0.1",
    "tap-only": "0.0.5",
    "then-sleep": "^1.0.1",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  },
  "snyk": true
}
